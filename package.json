{
  "name": "blue-dragon-motors-ai-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:analyze": "ng build --configuration=production --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "preview": "ng serve --configuration=production",
    "seed": "node scripts/seed.mjs",
    "seed:motorcycles": "node scripts/motorcycles/seed-motorcycles.mjs",
    "seed:oils": "node scripts/seed-oils.mjs",
    "seed:all": "npm run seed && npm run seed:motorcycles && npm run seed:oils",
    "ai-proxy": "cd server && npm start",
    "ai-proxy:dev": "cd server && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run ai-proxy:dev\"",
    "test:ai-proxy": "node test-ai-proxy.js",
    "build:docker": "docker build -t blue-dragon-motors:latest .",
    "deploy:local": "./scripts/deploy.sh local",
    "deploy:docker": "docker-compose up -d",
    "deploy:stop": "docker-compose down",
    "deploy:test": "./scripts/deploy.sh test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test": "jest",
    "test:ci": "jest --coverage --watchAll=false",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^20.1.0",
    "@angular/build": "^20.1.0",
    "@angular/cdk": "^20.2.5",
    "@angular/cli": "^20.1.0",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.1.6-0",
    "@angular/material": "^20.2.5",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-browser-dynamic": "^20.1.0",
    "@angular/router": "^20.1.6-0",
    "@angular/service-worker": "^20.3.3",
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@google/genai": "^1.19.0",
    "@modelcontextprotocol/sdk": "^1.18.1",
    "@zxing/browser": "0.1.5",
    "@zxing/library": "0.21.0",
    "angular-calendar": "^0.32.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "firebase": "^12.2.1",
    "ng2-charts": "^8.0.0",
    "openai": "^5.23.1",
    "rxjs": "^7.8.2",
    "tailwindcss": "latest",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cypress": "^13.6.0",
    "eslint": "^8.57.0",
    "imagemin-cli": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.1.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
